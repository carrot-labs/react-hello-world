{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "rules": {
		"indent": [2, "tab"]
		"quotes": [2, "single"],
		"linebreak-style": [2, "unix"],
		"semi": [2, "always"],
		"coma-dangle": [2, "never"],
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2
    },
    "plugins": ["react"]
}
